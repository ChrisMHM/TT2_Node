<%- include('../includes/head.ejs')%>
<body>
<!--Header section-->
<%- include('../includes/header-main-menu.ejs')%>

<!--    Section imagenes-->
<section class="form-container">
    <div class="container-fluid frame-form">
        <span class="icon-style">
            <i class="fas fa-file-image"></i>
        </span>
        <h3>Seleccionar y subir imágenes de las grietas</h3>
        <form>
            <div class="form-group">
                <label for="file-chooser" class="icon-style arrow"><i class="fas fa-arrow-up"></i></label>
                <input type="file" id="file-chooser" accept="image/png, image/jpeg" style="display: none;">
            </div>
            <div class="form-group">
                <label for="exampleFormControlSelect2">Imágenes seleccionadas</label>
                <select multiple class="form-control" id="exampleFormControlSelect2">
                    <option>1.jpg</option>
                    <option>2.jpg</option>
                    <option>3.jpg</option>
                    <option>4.jpg</option>
                    <option>5.jpg</option>
                </select>
            </div>
            <div class="row btn-container">
                <div class="col-md-6 col-6">
                    <a href="../main_menu/main-menu" class="btn btn-form">Regresar</a>
                </div>
                <div class="col-md-6 col-6">
                    <a href="./images-data" class="btn btn-form">Enviar</a>
                </div>
            </div>
        </form>
    </div>
</section>